<script>
	seajs.use(['init'],function(){

		var divBtn = $('.divBtn span');
		var iH = $('.divDetail .ih');

		$(window).resize(function(){

			divBtn.show().css({

				top:( divBtn.parent().outerHeight() - divBtn.height() )/2

			})

			iH.show().each(function(){

				$(this).css({
					top:( $(this).parent().outerHeight() - $(this).height() )/2
				})

			})

		}).resize()


		var targetLi = $('.caseList li');

		targetLi.on('mouseenter',function(){

			$(this).find('.masks').stop().animate({opacity:1})

		});

		targetLi.on('mouseleave',function(){

			$(this).find('.masks').stop().animate({opacity:0});

		});





		$('#down').on('click',function(){

			$('body,html').animate({
				scrollTop:$('#caseList').offset().top
			})

		});

		var func = $('#func');
		var onOffs = true;

		$(window).scroll(function(){

			var scrollTop = $(this).scrollTop();

			if(scrollTop > 300){

				func.find('.title-1').animate({
					opacity:1
				},600)

			}else{
				onOffs = true;
			}

			if(scrollTop > 600){

				func.find('.items-1 .imgBox').animate({
					opacity:1,
					marginTop:0
				},600);

				func.find('.items-1 .content').delay(300).animate({
					opacity:1,
					marginTop:0
				},800)

			}

			if(scrollTop > 900){

				func.find('.items-2 .content').animate({
					opacity:1,
					marginTop:0
				},600);

				func.find('.items-2 .imgBox').delay(300).animate({
					opacity:1,
					marginTop:0
				},800)

			}

			if(scrollTop > 1400){

				func.find('.title-2').animate({
					opacity:1
				},600)

			}

			if(scrollTop > 1800){

				func.find('.items-3 .imgBox').animate({
					opacity:1,
					marginTop:0
				},600);

				func.find('.items-3 .content').delay(300).animate({
					opacity:1,
					marginTop:0
				},800)

			}

			if(scrollTop > 2200){

				func.find('.items-4 .content').animate({
					opacity:1,
					marginTop:0
				},600);

				func.find('.items-4 .imgBox').delay(300).animate({
					opacity:1,
					marginTop:0
				},800)

			}


			if(onOffs){

				if(scrollTop > $('#caseList').offset().top - 560){

					onOffs = false;

					$('#down').click();
					
				}

			}


		})









	});
</script>


