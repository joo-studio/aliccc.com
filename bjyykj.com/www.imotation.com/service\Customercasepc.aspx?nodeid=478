<script>
	seajs.use(['init'],function(){

		var divBtn = $('.divBtn span');
		var iH = $('.divDetail .ih');

		$(window).resize(function(){

			divBtn.show().css({

				top:( divBtn.parent().outerHeight() - divBtn.height() )/2

			})

			iH.show().each(function(){

				$(this).css({
					top:( $(this).parent().outerHeight() - $(this).height() )/2
				})

			})

		}).resize()


		var targetLi = $('.caseList li');

		targetLi.on('mouseenter',function(){

			$(this).find('.masks').stop().animate({opacity:1})

		});

		targetLi.on('mouseleave',function(){

			$(this).find('.masks').stop().animate({opacity:0});

		});





		$('#down').on('click',function(){

			$('body,html').animate({
				scrollTop:$('#caseList').offset().top
			})

		})


		var createFunction = $('.createFunction');
		var onOffs = true;

		$(window).on('scroll',function(){

			var scrollTop = $(this).scrollTop();

			if(scrollTop > 500){


				createFunction.find('.ibox-1').animate({
					opacity:1
				},function(){

					createFunction.find('.ibox-2').animate({
						opacity:1
					},function(){

						createFunction.find('.ibox-3').animate({
							opacity:1
						},function(){

							createFunction.find('.ibox-4').animate({
								opacity:1
							},function(){

								createFunction.find('.ibox-5').animate({
									opacity:1
								},function(){

									createFunction.find('.ibox-6').animate({
										opacity:1
									},function(){
										
										createFunction.find('.ibox-7').animate({
											opacity:1
										})

									})

								})

							})

						})
					})

				})

			}else{

				onOffs = true;
			}

			if(onOffs){

				if(scrollTop > $('#caseList').offset().top - 480){

					onOffs = false;

					$('#down').click();
					
				}

			}

		});





	});
</script>
